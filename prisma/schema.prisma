// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  name     String
  password String
}

model AddProducts {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  name     String
  visitors Int
  price    Int
  sales    Int
  month    String
}

model AddVistors {
  id             String  @id @default(auto()) @map("_id") @db.ObjectId
  visitors       Int?
  premiumVistors Int?
  loaction       String?
  device         String?
  month          String?
}
